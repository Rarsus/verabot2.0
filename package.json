{
  "name": "verabot2-0",
  "displayName": "VeraBot2.0",
  "version": "0.1.0",
  "description": "VeraBot2.0 - Generic modular Discord bot",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "register-commands": "node src/register-commands.js",
    "test": "node ./scripts/run-tests.js",
    "test:quotes": "node ./scripts/test-quotes.js",
    "test:quotes-advanced": "node ./scripts/test-quotes-advanced.js",
    "test:utils:base": "node ./scripts/test-command-base.js",
    "test:utils:options": "node ./scripts/test-command-options.js",
    "test:utils:helpers": "node ./scripts/test-response-helpers.js",
    "test:integration:refactor": "node ./scripts/test-integration-refactor.js",
    "test:all": "npm test && npm run test:quotes && npm run test:quotes-advanced && npm run test:utils:base && npm run test:utils:options && npm run test:utils:helpers && npm run test:integration:refactor",
    "lint": "eslint --ext .js src/ scripts/",
    "ci": "npm ci && npm test"
  },
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "discord.js": "^14.11.0",
    "dotenv": "^16.3.1",
    "node-fetch": "^2.7.0",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "eslint": "^8.48.0"
  }
}
